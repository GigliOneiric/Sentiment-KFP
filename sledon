                    apiVersion: machinelearning.seldon.io/v1
                    kind: SeldonDeployment
                    metadata:
                      labels:
                        app: seldon
                      name: seldon-sentiment
                      namespace: foo
                    spec:
                      annotations:
                        project_name: NLP Pipeline
                        deployment_version: v1
                      name: seldon-sentiment
                      predictors:
                      - componentSpecs:
                        - spec:
                            containers:
